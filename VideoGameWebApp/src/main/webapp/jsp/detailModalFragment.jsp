<div class="modal fade" id="game-details-modal" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">
                    &times;
                </button>
            </div>
            <div class="modal-body">
                <h3 id="game-detail-id"></h3>
                <table id="game-details-table">
                    <tr>
                        <th>Title:</th>
                        <td id="game-detail-title"></td>
                    </tr>
                    <tr>
                        <th>Publisher:</th>
                        <td id="game-detail-publisher"></td>
                    </tr>
                    <tr>
                        <th>Genre:</th>
                        <td id="game-detail-genre"></td>
                    </tr>
                    <tr>
                        <th>Multiplayer?</th>
                        <td id="game-detail-multiplayer"></td>
                    </tr>
                    <tr>
                        <th>Rating:</th>
                        <td id="game-detail-rating"></td>
                    </tr>
                </table>
            </div>
            <div class="modal-footer">
                <button type="button" class="close" data-dismiss="modal">
                    Close
                </button>
            </div>
        </div>
    </div>
</div>